{
  "name": "@panacea/holon-cli",
  "version": "1.0.6",
  "main": "index.js",
  "license": "MIT",
  "repository": "github:panaceajs/holon-cli",
  "bin": {
    "holon": "bin/cli.js"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.38",
    "@babel/generator": "^7.0.0-beta.38",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.38",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.39",
    "@babel/plugin-syntax-jsx": "^7.0.0-beta.38",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0-beta.38",
    "@babel/template": "^7.0.0-beta.38",
    "@babel/types": "^7.0.0-beta.39",
    "ansi-colors": "^1.0.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babylon": "^7.0.0-beta.38",
    "camel-case": "^3.0.0",
    "concat-stream": "^1.6.0",
    "constant-case": "^2.0.0",
    "diff": "^3.4.0",
    "emphasize": "^1.5.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.1.0",
    "file-exists": "^5.0.1",
    "gulp-eslint": "^4.0.2",
    "gulp-tap": "^1.0.1",
    "inquirer": "^5.0.0",
    "jest-diff": "^22.1.0",
    "merge2": "^1.2.1",
    "pino": "^4.10.4",
    "pino-colada": "^1.4.3",
    "plugin-error": "^1.0.1",
    "prettier": "^1.10.2",
    "through2": "^2.0.3",
    "upper-case-first": "^1.1.2",
    "vinyl-fs": "^3.0.1",
    "yargs": "^10.1.1"
  },
  "devDependencies": {
    "babel-plugin-tester": "^5.0.0",
    "eslint-junit": "^1.0.1",
    "html-jest-reporter": "^1.0.0",
    "jest": "^22.2.1",
    "jest-html-reporter": "^1.2.0",
    "jest-junit": "^3.6.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint -c ./.eslintrc.js ./src",
    "lint-report": "eslint -c ./.eslintrc.js -f ./node_modules/eslint-junit/index.js ./src"
  },
  "majestic": {
    "runScript": "jest --silent"
  },
  "jest": {
    "collectCoverage": true,
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "testResultsProcessor": "./jest-results-processor.js",
    "testPathIgnorePatterns": [
      "/templates/",
      "/_temp/"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov",
      "html"
    ],
    "reporters": [
      "default"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./reports/jest-junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "eslint-junit": {
    "suiteName": "eslint tests",
    "output": "./reports/eslint-junit.xml",
    "classNameTemplate": "{ruleId}",
    "titleTemplate": "{line} : {source}"
  },
  "jest-html-reporter": {
    "pageTitle": "Your test suite",
    "outputPath": "./reports/jest-html-reporter-test-report.html",
    "includeFailureMsg": true
  },
  "html-jest-reporter": {
    "pageTitle": "Your test suite",
    "outputPath": "./reports/html-jest-reporter-test-report.html",
    "includeFailureMsg": true
  }
}
